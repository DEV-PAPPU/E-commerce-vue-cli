 axios from 'axios'\n\n   export default {\n        metaInfo: {\n        title: 'Rigister Page',\n        },\n\n     data: () => ({\n       errors:{},\n       form: {\n          name: '',\n          email: '',\n          password: '',\n          password_confirmation: ''\n      },\n\n      }),\n        methods: {\n           rigister() {\n                axios.post('auth/register',this.form).then(response => {\n                   console.log(response)\n                   this.$router.push({name:'Login'});\n                 \n                })\n                .catch(e =>{\n                    this.errors = e.response.data\n                })\n            },\n\n        },\n    }\n</script>\n\n\n\n"]}]}